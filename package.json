{
    "name": "react-native-formik-json",
    "version": "0.0.1",
    "description": "react-native-formik-json is a modification of formik-json to work on React native",
    "main": "src/index.js",
    "files": [
        "css",
        "es",
        "lib",
        "umd"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/flipbyte/formik-json-schema.git"
    },
    "scripts": {
        "watch": "nwb serve",
        "build": "nwb build-react-component --copy-files",
        "clean": "nwb clean-module && nwb clean-demo",
        "prepublishOnly": "npm run build",
        "start": "nwb serve-react-demo",
        "test": "nwb test-react",
        "test:coverage": "nwb test-react --coverage",
        "test:watch": "nwb test-react --server",
        "lint-md": "remark .",
        "publish-demo": "npm run build && gh-pages -d demo/dist"
    },
    "dependencies": {
        "@babel/runtime": "^7.15.3",
        "@flipbyte/when-condition": "^0.6.0",
        "@flipbyte/yup-schema": "^0.1.5",
        "codemirror": "^5.62.3",
        "formik": "^2.2.9",
        "react-autosuggest": "^10.1.0",
        "react-codemirror2": "^7.2.1",
        "react-dropzone": "^11.3.4",
        "react-quill": "^1.3.5",
        "react-select": "^4.3.1",
        "react-sortable-hoc": "^2.0.0",
        "react-switch": "^6.0.0",
        "shallowequal": "^1.1.0"
    },
    "peerDependencies": {
        "@fortawesome/fontawesome-free": "^5.15.4",
        "lodash": "^4.17.21",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^5.15.4",
        "axios": "^0.21.1",
        "babel-plugin-transform-import-styles": "^0.0.11",
        "bootstrap": "^5.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "gh-pages": "^3.2.3",
        "inferno": "^7.4.9",
        "load-styles": "^2.0.0",
        "nwb": "^0.25.2",
        "react": "^17.0.2",
        "react-native": "^0.65.1",
        "remark-lint": "^9.0.1"
    },
    "author": "",
    "homepage": "",
    "license": "MIT",
    "keywords": [
        "react-json-form",
        "react-json-schema-form",
        "json-schema-form",
        "json-form",
        "formik-json",
        "formik-schema",
        "schema-form",
        "form",
        "validation",
        "bootstrap"
    ],
    "remarkConfig": {
        "plugins": [
            "remark-preset-lint-recommended"
        ]
    }
}
